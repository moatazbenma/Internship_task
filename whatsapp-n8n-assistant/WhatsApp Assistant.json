    {
      "parameters": {
        "jsCode": "// Get the message from the Webhook node\nconst body = $node[\"Webhook\"].json[\"body\"][\"Body\"] || \"\"; // e.g. \"DELETE /TestFolder/file.pdf\"\n\n// Extract folder name and file name using regex\nconst match = body.match(/^DELETE\\s+\\/([^\\/]+)\\/(.+)$/i);\n\nif (!match) {\n  return [{\n    json: { error: \"‚ùå Invalid format. Use: DELETE /FolderName/FileName\" }\n  }];\n}\n\nconst folderName = match[1].trim();\nconst fileName = match[2].trim();\n\n// Search among folders returned by the previous node\nconst folders = $input.all();\nconst matchingFolder = folders.find(item => {\n  return item.json.name?.toLowerCase() === folderName.toLowerCase();\n});\n\nif (!matchingFolder) {\n  return [{\n    json: { error: `‚ùå Folder \"${folderName}\" not found.` }\n  }];\n}\n\n// Return folderId, folderName, and fileName\nreturn [{\n  json: {\n    folderId: matchingFolder.json.id,\n    folderName: matchingFolder.json.name,\n    fileName: fileName\n  }\n}];\n"
      },
